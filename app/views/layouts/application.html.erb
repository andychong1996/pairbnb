<!DOCTYPE html>
<html>
<head>
  <title>Pairbnb</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <meta name="viewport" content="width=device-width, initial-scale=1">

  
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery-form-validator/2.2.8/jquery.form-validator.min.js"></script>
  <link href="//cdnjs.cloudflare.com/ajax/libs/jquery-form-validator/2.2.8/theme-default.min.css"
  rel="stylesheet" type="text/css" />
</head>
<body>
  <nav>
    <% if !user_logged_in? %>
      <%= link_to "PairBnB", root_path %>
      <%= link_to "Log In", new_session_path %>
      <%= link_to 'Sign In with Facebook', '/auth/facebook' %>
      <%= link_to "Sign Up", sign_up_form_path %>
    <% else %>
      <%= link_to "PairBnB", root_path %>
      <%= link_to "Log Out", session_path(current_user), method: :delete %>
    <% end %>
  </nav>
  <div>
    <% flash.each do |k, v| %>
      <p><%= "#{v}" %></p>
    <% end %>
  </div>
  <%= yield %>

</body>
</html>
