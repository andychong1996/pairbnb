<div class="navbar-buffer">

</div>

<header>
  <h1><%= current_user.first_name.capitalize+" "+current_user.last_name.capitalize + " Profile"%></h1>
  <span><%= link_to "Edit Profile", edit_user_path %></span>
</header>

<div class="profile-body">
  <% unless current_user.avatar_url.nil? %>
    <%= image_tag current_user.avatar_url(:display) %>
    <br />
  <% end %>
  <div class="profile-content">
    <h3>First Name: <span><%= current_user.first_name %></span></h3>
  </div>
</div>
